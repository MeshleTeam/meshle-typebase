{
  "name": "meshle-typebase",
  "description": "Package and unpackage binary structs",
  "version": "0.5.8",
  "keywords": [
    "struct",
    "type",
    "variable",
    "pointer",
    "array",
    "list",
    "native",
    "native-code",
    "machine-code",
    "compiler"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jfamousket/meshle-typebase"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "docs": "rm -rf docs && docco --output ./docs --layout linear index.ts",
    "build:js": "tsc index.ts",
    "build": "yarn build:types && yarn build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "prepublish": "yarn build"
  },
  "devDependencies": {
    "@types/node": "^13.9.3"
  },
  "dependencies": {
    "buffer": "^5.5.0"
  }
}
